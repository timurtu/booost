{
  "name": "booost",
  "version": "0.0.1",
  "description": "Mobile runner game that switches dimensions.",
  "main": "main.js",
  "scripts": {
    "install": "bower install",
    "start": "npm-run-all build --parallel watch electron",
    "electron": "electron ./dist/node/main.js",
    "build": "gulp build",
    "watch": "gulp watch",
    "test": "ava --watch",
    "package": "electron-packager . --all --overwrite --no-prune",
    "mas": "electron-packager . --platform=mas --overwrite --no-prune --osx-sign"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timurtu/adventure-game.git"
  },
  "keywords": [
    "booost",
    "2D",
    "3D",
    "dimension",
    "switching"
  ],
  "author": "Timur Uzel <timurt.uzel@gmail.com> (https://github.com/timurtu)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timurtu/booost/issues"
  },
  "homepage": "https://github.com/timurtu/booost#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2016": "^6.11.3",
    "bcrypt": "^0.8.7",
    "bluebird": "^3.4.6",
    "bower": "^1.7.9",
    "craftyjs": "^0.7.1",
    "css-loader": "^0.24.0",
    "deep-freeze": "0.0.1",
    "domali": "^2.1.1",
    "electron": "^1.4.0",
    "electron-packager": "^8.0.0",
    "electron-prebuilt": "^1.4.0",
    "electron-rebuild": "^1.2.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gutil-color-log": "^1.0.2",
    "node-sass": "^3.8.0",
    "npm-run-all": "^3.1.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.1",
    "spectron": "^3.4.0",
    "stats.js": "^0.16.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.4.6",
    "mongoose": "^4.6.0"
  }
}
